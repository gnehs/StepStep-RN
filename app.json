{
  "expo": {
    "name": "餅餅踏踏",
    "slug": "餅餅踏踏",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f2f2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "tw.pancake.stepstep"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f2f2f2"
      },
      "package": "tw.pancake.stepstep"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 26
          }
        }
      ],
      "react-native-health-connect"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
